{
  "name": "Reservations",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "setup-android": "react-native bundle --platform android --dev true --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "setup-ios": "react-native bundle --platform ios --dev true --entry-file index.ios.js --bundle-output android/app/src/main/assets/index.ios.bundle --assets-dest ios/app/src/main/res && react-native run-ios"
  },
  "dependencies": {
    "@types/jest": "^24.0.12",
    "apollo-boost": "^0.3.1",
    "enzyme": "^3.9.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "jest-serializer-enzyme": "^1.0.0",
    "native-base": "^2.12.1",
    "react": "16.8.3",
    "react-addons-test-utils": "^15.6.2",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-native": "0.59.5",
    "react-native-gesture-handler": "^1.1.0",
    "react-navigation": "^3.9.1",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-jest": "^24.7.1",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "mock-apollo-client": "^0.2.0",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-navigation|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
    ],
    "setupFiles": [
      "./setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  }
}
